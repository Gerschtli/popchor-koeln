{
    "name": "popchor-koeln",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@8.6.6",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "storyblok:pull-components": "pnpx storyblok pull-components --space 227856 && storyblok-generate-ts source=./components.227856.json target=./src/lib/component-types-storyblok && pnpm run format",
        "storyblok:push-components": "pnpx storyblok push-components --space 227856 components.227856.json"
    },
    "devDependencies": {
        "@playwright/test": "1.27.1",
        "@storyblok/svelte": "^3.0.11",
        "@sveltejs/adapter-netlify": "^2.0.8",
        "@sveltejs/kit": "^1.22.2",
        "@types/nodemailer": "^6.4.8",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-svelte": "^2.32.2",
        "lucide-svelte": "^0.259.0",
        "nodemailer": "^6.9.3",
        "postcss": "^8.4.25",
        "postcss-load-config": "^4.0.1",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "prettier-plugin-tailwindcss": "^0.5.0",
        "storyblok-generate-ts": "^1.15.0",
        "svelte": "^4.0.5",
        "svelte-check": "^3.4.6",
        "svelte-preprocess": "^5.0.4",
        "sveltekit-superforms": "^1.2.0",
        "tailwindcss": "^3.3.2",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6",
        "vite": "^4.4.3",
        "vite-imagetools": "^5.0.5",
        "zod": "^3.21.4"
    },
    "type": "module"
}
