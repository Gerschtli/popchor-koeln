        "prettier-plugin-tailwindcss": "^0.6.11",
        "storyblok": "^3.36.0",
        "svelte": "^5.28.2",
        "svelte-check": "^4.1.6",
        "sveltekit-superforms": "^2.24.1",
        "tailwind-merge": "^3.2.0",
        "tailwindcss": "^4.1.4",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.31.0",
        "vite": "^6.3.3",
        "vite-imagetools": "^7.0.5",
        "zod": "^3.24.3"
