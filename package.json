{
    "name": "popchor-koeln",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@9.4.0",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
        "format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
        "storyblok:pull-components": "pnpx storyblok pull-components --space 227856 && storyblok-generate-ts source=./components.227856.json target=./src/lib/component-types-storyblok && pnpm run format",
        "storyblok:push-components": "pnpx storyblok push-components --space 227856 components.227856.json"
    },
    "devDependencies": {
        "@playwright/test": "1.45.2",
        "@storyblok/svelte": "^4.0.6",
        "@sveltejs/adapter-netlify": "^4.3.1",
        "@sveltejs/kit": "^2.5.18",
        "@sveltejs/vite-plugin-svelte": "^3.1.1",
        "@types/nodemailer": "^6.4.15",
        "@typescript-eslint/eslint-plugin": "^7.16.1",
        "@typescript-eslint/parser": "^7.16.1",
        "autoprefixer": "^10.4.19",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-playwright": "^1.6.2",
        "eslint-plugin-svelte": "^2.42.0",
        "lucide-svelte": "^0.436.0",
        "nodemailer": "^6.9.14",
        "postcss": "^8.4.39",
        "postcss-load-config": "^5.1.0",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.6",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "storyblok-generate-ts": "^2.1.0",
        "svelte": "^4.2.18",
        "svelte-check": "^3.8.4",
        "svelte-preprocess": "^6.0.2",
        "sveltekit-superforms": "^2.16.0",
        "tailwind-merge": "^2.4.0",
        "tailwindcss": "^3.4.6",
        "tslib": "^2.6.3",
        "typescript": "^5.5.3",
        "vite": "^5.3.4",
        "vite-imagetools": "^7.0.4",
        "zod": "^3.23.8"
    },
    "type": "module"
}
