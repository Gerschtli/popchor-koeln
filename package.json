    "packageManager": "pnpm@8.8.0",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
        "format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
        "storyblok:pull-components": "pnpx storyblok pull-components --space 227856 && storyblok-generate-ts source=./components.227856.json target=./src/lib/component-types-storyblok && pnpm run format",
        "storyblok:push-components": "pnpx storyblok push-components --space 227856 components.227856.json"
    },
    "devDependencies": {
        "@playwright/test": "1.39.0",
        "@storyblok/svelte": "^3.1.6",
        "@sveltejs/adapter-netlify": "^2.0.8",
        "@sveltejs/kit": "^1.25.2",
        "@types/nodemailer": "^6.4.11",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "@vitejs/plugin-basic-ssl": "^1.0.1",
        "autoprefixer": "^10.4.16",
        "dotenv": "^16.3.1",
        "drizzle-kit": "^0.19.13",
        "drizzle-orm": "^0.28.6",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-playwright": "^0.17.0",
        "eslint-plugin-svelte": "^2.34.0",
        "lucide-svelte": "^0.292.0",
        "nodemailer": "^6.9.6",
        "pg": "^8.11.3",
        "postcss": "^8.4.31",
        "postcss-load-config": "^4.0.1",
        "postgres": "^3.4.0",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.6",
        "storyblok-generate-ts": "^1.15.1",
        "svelte": "^4.2.1",
        "svelte-check": "^3.5.2",
        "svelte-preprocess": "^5.0.4",
        "sveltekit-superforms": "^1.8.0",
        "tailwind-merge": "^1.14.0",
        "tailwindcss": "^3.3.3",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^4.4.11",
