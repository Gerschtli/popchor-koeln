{
    "name": "popchor-koeln",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@8.6.12",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
        "format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
        "storyblok:pull-components": "pnpx storyblok pull-components --space 227856 && storyblok-generate-ts source=./components.227856.json target=./src/lib/component-types-storyblok && pnpm run format",
        "storyblok:push-components": "pnpx storyblok push-components --space 227856 components.227856.json"
    },
    "devDependencies": {
        "@playwright/test": "1.27.1",
        "@storyblok/svelte": "^3.1.6",
        "@sveltejs/adapter-netlify": "^2.0.8",
        "@sveltejs/kit": "^1.25.0",
        "@types/nodemailer": "^6.4.10",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "autoprefixer": "^10.4.15",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-svelte": "^2.33.1",
        "lucide-svelte": "^0.279.0",
        "nodemailer": "^6.9.5",
        "postcss": "^8.4.29",
        "postcss-load-config": "^4.0.1",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.4",
        "storyblok-generate-ts": "^1.15.1",
        "svelte": "^4.2.0",
        "svelte-check": "^3.5.1",
        "svelte-preprocess": "^5.0.4",
        "sveltekit-superforms": "^1.6.1",
        "tailwindcss": "^3.3.3",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
        "vite-imagetools": "^5.0.8",
        "zod": "^3.22.2"
    },
    "type": "module"
}
