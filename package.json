{
    "name": "popchor-koeln",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@9.12.1",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
        "format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
        "storyblok:pull-components": "pnpx storyblok pull-components --space 227856 && storyblok-generate-ts source=./components.227856.json target=./src/lib/component-types-storyblok && pnpm run format",
        "storyblok:push-components": "pnpx storyblok push-components --space 227856 components.227856.json"
    },
    "devDependencies": {
        "@playwright/test": "1.49.1",
        "@storyblok/svelte": "^4.0.11",
        "@sveltejs/adapter-netlify": "^4.4.2",
        "@sveltejs/kit": "^2.20.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@types/nodemailer": "^6.4.17",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "autoprefixer": "^10.4.21",
        "bigger-picture": "^1.1.16",
        "dotenv": "^16.4.7",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-playwright": "^2.2.0",
        "eslint-plugin-svelte": "^2.46.1",
        "globals": "^16.0.0",
        "lucide-svelte": "^0.482.0",
        "nodemailer": "^6.10.0",
        "postcss": "^8.5.3",
        "postcss-load-config": "^6.0.1",
        "prettier": "^3.5.3",
        "prettier-plugin-svelte": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "storyblok-generate-ts": "^2.2.0",
        "svelte": "^5.23.1",
        "svelte-check": "^4.1.5",
        "svelte-preprocess": "^6.0.3",
        "sveltekit-superforms": "^2.24.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss": "^3.4.17",
        "tslib": "^2.8.1",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.1",
        "vite": "^6.2.2",
        "vite-imagetools": "^7.0.5",
        "vite-plugin-svelte:^3": "link:@sveltejs/vite-plugin-svelte:^3",
        "zod": "^3.24.2"
