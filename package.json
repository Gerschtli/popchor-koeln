{
    "name": "popchor-koeln",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@9.12.1",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
        "format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
        "storyblok:pull-components": "pnpx storyblok pull-components --space 227856 && storyblok-generate-ts source=./components.227856.json target=./src/lib/component-types-storyblok && pnpm run format",
        "storyblok:push-components": "pnpx storyblok push-components --space 227856 components.227856.json"
    },
    "devDependencies": {
        "@playwright/test": "1.48.1",
        "@storyblok/svelte": "^4.0.9",
        "@sveltejs/adapter-netlify": "^4.3.5",
        "@sveltejs/kit": "^2.7.2",
        "@sveltejs/vite-plugin-svelte": "^3.1.2",
        "@types/nodemailer": "^6.4.16",
        "@typescript-eslint/eslint-plugin": "^8.10.0",
        "@typescript-eslint/parser": "^8.10.0",
        "autoprefixer": "^10.4.20",
        "dotenv": "^16.4.5",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-playwright": "^1.8.0",
        "eslint-plugin-svelte": "^2.45.1",
        "globals": "^15.11.0",
        "lucide-svelte": "^0.468.0",
        "nodemailer": "^6.9.15",
        "postcss": "^8.4.47",
        "postcss-load-config": "^6.0.1",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.7",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "storyblok-generate-ts": "^2.1.0",
        "svelte": "^4.2.19",
        "svelte-check": "^4.0.5",
        "svelte-preprocess": "^6.0.3",
        "sveltekit-superforms": "^2.20.0",
        "tailwind-merge": "^2.5.4",
        "tailwindcss": "^3.4.14",
        "tslib": "^2.8.0",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.10.0",
        "vite": "^5.4.9",
        "vite-imagetools": "^7.0.4",
        "vite-plugin-svelte:^3": "link:@sveltejs/vite-plugin-svelte:^3",
        "zod": "^3.23.8"
    },
    "type": "module"
